  pod 'FirebaseUI'
  pod 'FirebaseUI/Google'
  pod 'FirebaseUI/Facebook'
  pod 'FirebaseUI/OAuth' # Used for Sign in with Apple, Twitter, etc
  pod 'FirebaseUI/Phone'

  pod 'Firebase/Storage'
  pod 'Firebase/Firestore'

  # Pods for AlarmTest

end
